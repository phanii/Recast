<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="switchCompactTheme">
        <item name="android:colorControlActivated">#f00</item>
    </style>

    <style name="switchcompactStyle">
        <item name="android:thumb">@drawable/switchthumb</item>
        <item name="android:trackTint">@color/switch_color</item>
    </style>

    <style name="CustomPopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/custom_anim_from_top</item>
        <item name="android:windowExitAnimation">@anim/custom_anim_to_top</item>
    </style>

</resources>
